      <label backgroundcolor="0xffffff" borderalpha="1" wordwrap="1" textalpha="1" textcolor="0x000000" background="1" bordercolor="0x000000" border="1" width="180" backgroundalpha="1" enabled="1" borderradius="1" height="20"/>
